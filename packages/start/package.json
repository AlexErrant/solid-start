    "./root/*": "./root/*",
    "./data": "./data/index.tsx",
    "./data/*": "./data/*",
    "./node/*": "./node/*",
    "./fs-router/*": "./fs-router/*",
    "./dev/*": "./dev/*",
    "./server": {
      "browser": "./server/browser.ts",
      "node": "./server/server.ts",
      "default": "./server/server.ts"
    },
    "./server/*": "./server/*",
    "./session": "./session/index.ts",
    "./session/*": "./session/*"
