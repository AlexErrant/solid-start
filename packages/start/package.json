    "solid-start-cloudflare-workers": "0.1.0-alpha.95",
    "solid-start-deno": "0.1.0-alpha.95",
    "solid-start-netlify": "0.1.0-alpha.95",
    "solid-start-node": "0.1.0-alpha.95",
    "solid-start-static": "0.1.0-alpha.95",
